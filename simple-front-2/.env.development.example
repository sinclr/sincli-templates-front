# frontend/.env.development.example

# Application
VITE_APP_NAME=${APP_NAME}
VITE_APP_VERSION=0.0.1
VITE_APP_ENV=development

# API - Sera mis à jour après le déploiement AWS
VITE_API_URL=http://localhost:8000

# API Gateway AWS (sera remplacé après sincli app:setup-aws)
# VITE_API_URL=https://YOUR-API-ID.execute-api.eu-west-3.amazonaws.com

# Feature Flags - Tout activé pour le dev
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_SHOW_DEV_TOOLS=true
VITE_ENABLE_MOCK_DATA=true

# Clés publiques de test - À remplir si nécessaire
VITE_STRIPE_PUBLIC_KEY=
VITE_GOOGLE_MAPS_KEY=

# Cognito (optionnel) - Sera mis à jour après sincli app:setup-aws
VITE_COGNITO_USER_POOL_ID=
VITE_COGNITO_CLIENT_ID=
VITE_AWS_REGION=eu-west-3